	<service id="getCTLComeCode" label="기계가공 공법 코드 조회">
		<script>
			<![CDATA[
			try{
				var resultList = $jdbc.queryList("bp/bp/tcams/cc/cc01", "ctl_comecode.list.select", $data.param)
				var procComboList = new List();
				var cannComboList = new List();
				for(var i=0; i<resultList.size(); i++){
					if(resultList[i].gubn == "PROC"){
						procComboList.add(resultList[i]);
					}else{
						cannComboList.add(resultList[i]);
					}
				}
				$data.procComboList = procComboList;
				$data.cannComboList = cannComboList;
				
			}catch(e){
				System.out.println(e);
				var rollback = new Map();
				$jdbc.update("bp/tcams/ca/common", "rollback", rollback);
			}
			
			
			]]>
		</script>
	</service>
	
