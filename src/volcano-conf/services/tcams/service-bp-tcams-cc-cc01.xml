 <service id="get.ca_come_code.list" label="공법코드 조회">
  <script>
   <![CDATA[
    var ret = new Map();
    
    try {
        $data.result = $jdbc.queryList("bp/bp/tcams/cc/cc01", "ca_come_code.list.select", $data.param)
    
        var chk = $jdbc.queryList("bp/tcams/ci/ci03/conf/USER", "select.work", $data.param);
        $data.param.curr_come = chk[0].curr_come;
    	
    	ret.code = "Y";
    	ret.mesg = "OK";
    } catch(e) {
    	$logger.info(e.message)
    	ret.code = "N";
    	ret.mesg = e.message;
    }
    
    $data.ret = ret;
    
   ]]>
  </script>
 </service>

